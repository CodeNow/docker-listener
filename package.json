{
  "name": "docker-listener",
  "version": "0.3.0",
  "description": "Pipe Docker events into external target",
  "main": "main.js",
  "private": true,
  "scripts": {
    "test-watch": "node_modules/.bin/rerun-script test",
    "test": "clear; npm run lint && NODE_PATH=./lib NODE_ENV=test lab -v -m 3000 -e test -f test/",
    "lint": "jshint .",
    "start": "NODE_PATH=./lib node index.js"
  },
  "watches": {
    "test": [
      "*.js",
      "lib/**/*.js",
      "test/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeNow/docker-listener.git"
  },
  "keywords": [
    "docker",
    "events"
  ],
  "author": "Runnable",
  "bugs": {
    "url": "https://github.com/CodeNow/docker-listener/issues"
  },
  "homepage": "https://github.com/CodeNow/docker-listener",
  "dependencies": {
    "101": "^0.7.1",
    "boom": "^2.6.0",
    "debug": "^2.1.0",
    "dockerode": "^2.0.7",
    "eson": "^0.5.0",
    "exec-sync": "^0.1.6",
    "express": "^4.10.2",
    "ip": "^0.3.2",
    "keypather": "^1.10.1",
    "loadenv": "git://github.com/runnable/loadenv#4bd01e514032bffecb65102da479964e0317d728",
    "monitor-dog": "^1.1.2",
    "morgan": "^1.5.0",
    "node-uuid": "^1.4.1",
    "redis": "^0.12.1",
    "rollbar": "^0.3.13",
    "runnable-hermes": "^1.0.0"
  },
  "devDependencies": {
    "callback-count": "^0.1.0",
    "code": "^1.2.1",
    "docker-mock": "0.4.0",
    "jshint": "^2.5.10",
    "lab": "^5.0.2",
    "rerun-script": "^0.6.0",
    "sinon": "^1.14.1",
    "supertest": "^0.15.0"
  }
}
