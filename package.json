{
  "name": "docker-listener",
  "version": "2.0.0",
  "description": "Pipe Docker events into external target",
  "main": "main.js",
  "private": true,
  "scripts": {
    "test-watch": "node_modules/.bin/rerun-script test",
    "test": "clear; npm run lint && npm run unit",
    "lint": "jshint .",
    "unit": "NODE_PATH=./lib lab -v -m 3000 -f test/ -a code",
    "start": "ulimit -c unlimited && NODE_PATH=./lib node --abort-on-uncaught-exception index.js"
  },
  "watches": {
    "test": [
      "*.js",
      "lib/**/*.js",
      "test/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeNow/docker-listener.git"
  },
  "keywords": [
    "docker",
    "events",
    "listener"
  ],
  "author": "Runnable",
  "bugs": {
    "url": "https://github.com/CodeNow/docker-listener/issues"
  },
  "homepage": "https://github.com/CodeNow/docker-listener",
  "dependencies": {
    "101": "^1.0.0",
    "async": "^0.9.0",
    "boom": "^2.6.0",
    "bunyan": "^1.4.0",
    "bunyan-loggly": "0.0.5",
    "bunyan-redis": "^0.1.4",
    "dockerode": "^2.0.7",
    "error-cat": "^1.4.0",
    "exec-sync": "^0.1.6",
    "express": "^4.10.2",
    "ip": "^0.3.2",
    "keypather": "^1.10.1",
    "loadenv": "^2.1.0",
    "monitor-dog": "^1.4.1",
    "morgan": "^1.5.0",
    "node-uuid": "^1.4.1",
    "runnable-hermes": "git://github.com/Runnable/hermes.git#v6.4.0"
  },
  "devDependencies": {
    "callback-count": "^0.1.0",
    "code": "^1.3.0",
    "docker-mock": "0.4.0",
    "jshint": "^2.5.10",
    "lab": "^5.8.0",
    "rerun-script": "^0.6.0",
    "rewire": "^2.3.3",
    "sinon": "^1.14.1",
    "supertest": "^0.15.0"
  }
}
